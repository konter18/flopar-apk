{
  "expo": {
    "name": "flopar",
    "slug": "flopar",
    "owner": "konter18", 
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/camion.png",
    "scheme": "flopar",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flopar.transportes"
    },
    "android": {
      "package": "com.flopar.transportes",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9bcbca8b-9462-42b6-8267-36937bca5ee3"
      }
    }
  }
}
